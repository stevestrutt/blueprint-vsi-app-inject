{
    "name": "Blueprint VSI App",
    "schema_version": "1.0.0",
    "source": {
        "source_type": "git_hub",
        "git": {
            "git_repo_url": "https://github.com/Cloud-Schematics/blueprint-vsi-app-inject",
            "git_repo_folder": "vsi-app-blueprint.yaml",
            "git_branch": "main"
        }
    },
    "config": [
        {
            "source": {
                "source_type": "git_hub",
                "git": {
                    "git_repo_url": "https://github.com/Cloud-Schematics/blueprint-vsi-app-inject",
                    "git_repo_folder": "vsi-app-input.yaml",
                    "git_branch": "main"
                }
            }
        }
    ],
    "inputs": [
        {
            "name": "provision_rg",
            "value": "true"
        },
        {
            "name": "resource_group_name",
            "value": "my-vsi-rg1"
        },
        {
            "name": "ssh_public_key",
            "value": "ssh-xxxxxx"
        }
    ],
    "description": "Deploys a simple vsi app blueprint",
    "resource_group": "Default"
}